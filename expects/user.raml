#%RAML 1.0
title: User API expected by auth-facebok
version: v1
mediaType: application/json
baseUri: hb://user

types:
  User:
    properties:
      user_id: string
      facebook_user_id?: string

/users:
  get:
    queryParameters:
      fields?: string[]
    responses:
      200:
        body: User[]